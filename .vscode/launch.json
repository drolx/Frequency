{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/src/Proton.Frequency/bin/Debug/net7.0/linux-x64/proton-frequency.dll",
            "args": [],
            "cwd": "${workspaceRoot}/src/Proton.Frequency/",
            "stopAtEntry": false,
            "console": "internalConsole",
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceRoot}/Views"
            }
        }

    ]
}