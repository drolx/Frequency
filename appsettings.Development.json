{
  "WebPort": 17250,
  "PersistData": true,
  "PersistDurationDays": 20,
  "AllowedHosts": "*",
  "Logging": {
    "Enable": true,
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Database": {
    "Store": "Data Source=app.db;Version=3;cache=shared",
    "Redis": {
      "Host": "127.0.0.1",
      "Port": 6379,
      "Instance": "master"
    },
    "InMemory": "Data Source=:memory:"
  },
  "IOTMode": {
    "Enable": true,
    "PushProcessCount": 1,
    "UniqueId": "OGUN001",
    "Protocol": "KingJoin",
    "RemoteHostUrl": "http://localhost:4444/iot/rfid",
    "RemoteHostMethod": "POST",
    "RemoteHostUsername": "user",
    "RemoteHostPassword": "pass",
    "MinRemoteHostDelay": 0.5,
    "MinRepeatFrequency": 30,
    "AutoRead": true,
    "Connections": {
      "Serial": {
        "Enable": true,
        "PortName": "null",
        "BaudRate": 9600,
        "DataBits": 8,
        "ConnectionRetries": 5,
        "ConnectionTimeout": 3
      },
      "Network": {
        "Enable": false,
        "Type": "TCP",
        "Address": "0.0.0.0",
        "Port": 56000,
        "ConnectionRetries": 5,
        "ConnectionTimeout": 3
      }
    },
    "NetworkCheck": true,
    "NetworkCheckAddress": "adapt.mcpl.ga",
    "NetworkCheckTimeout": 100
  },
  "CardOptions": {
    "Allowed": [
      "EPC-GEN1",
      "EPC-GEN2",
      "ISO1800-6B"
    ],
    "TruncateNumber": 0,
    "MaximumLength":  24
  },
  "ServerMode": {
    "Enable": false,
    "Store": true,
    "Protocols": [
      {
        "ProtocolName": "Chafon",
        "Type": "TCP",
        "Port": 17301
      },
      {
        "ProtocolName": "KingJoin",
        "Type": "UDP",
        "Port": 17302
      }
    ]
  }
}
