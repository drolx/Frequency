@using Microsoft.AspNetCore.Mvc.TagHelpers
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    @* <meta property="og:url" content="https://github.com/gpproton/"> *@
    @* <meta property="og:image" content="https://github.com/gpproton/test.png"> *@
    <meta property="og:locale" content="en_US">
