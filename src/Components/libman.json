{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "@fontsource/roboto@4.5.8",
      "destination": "wwwroot/lib/roboto"
    },
    {
      "library": "font-awesome@6.2.0",
      "destination": "wwwroot/lib/font-awesome",
      "files": [
        "css/all.min.css",
        "webfonts/*"
      ]
    },
    {
      "library": "mdb-ui-kit@5.0.0",
      "destination": "wwwroot/lib/mdb-ui-kit",
      "files": [
        "mdb.min.css",
        "mdb.min.js"
      ]
    },
    {
      "library": "Chart.js@3.9.1",
      "destination": "wwwroot/lib/Chart.js",
      "files": [
        "chart.min.js"
      ]
    }
  ]
}